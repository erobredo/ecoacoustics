git clone https://github.com/yuntu-org/yuntu.git && python3 -m pip install yuntu/src
wget --load-cookies /tmp/cookies.txt "https://docs.google.com/uc?export=download&confirm=$(wget --quiet --save-cookies /tmp/cookies.txt --keep-session-cookies --no-check-certificate 'https://docs.google.com/uc?export=download&id=FILEID' -O- | sed -rn 's/.*confirm=([0-9A-Za-z_]+).*/\1\n/p')&id=1BiXFOZMGsh7AXG_C29VTPZwmDuiRRH8l" -O BATMX_full.zip && rm -rf /tmp/cookies.txt
unzip BATMX_full.zip && rm BATMX_full.zip
svn export https://github.com/erobredo/ecoacoustics_data.git/trunk/1 data/1

